factory PML_paragraph_node_HTML_writer type:PML_node_HTML_writer<PML_paragraph_node>

    function write
    
        context.write_block_node_with_child_nodes (
            i_node,
            HTML_tag_name = "p"
            CSS_class_name = "pml-paragraph"
			append_new_line_for_tag = no )
    .
    
    creator create kind:in_all
.
