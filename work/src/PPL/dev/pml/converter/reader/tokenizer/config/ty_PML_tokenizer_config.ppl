type PML_tokenizer_config

    attributes type:character
        
        node_start_symbol default: se_PML_tokenizer_config_default_values.node_start_symbol
        node_end_symbol default: se_PML_tokenizer_config_default_values.node_end_symbol

        attributes_start_symbol default: se_PML_tokenizer_config_default_values.attributes_start_symbol
        attributes_end_symbol default: se_PML_tokenizer_config_default_values.attributes_end_symbol
        attribute_assignment_symbol default: se_PML_tokenizer_config_default_values.attribute_assignment_symbol
    .

    attributes type:string
        
        comment_start default: se_PML_tokenizer_config_default_values.comment_start
        comment_end   default: se_PML_tokenizer_config_default_values.comment_end

        insert_file_start default: se_PML_tokenizer_config_default_values.insert_file_start
    .

    attributes type:regex

        node_tag default: se_PML_tokenizer_config_default_values.node_tag
        
        attribute_id default: se_PML_tokenizer_config_default_values.attribute_id
        attribute_id_and_assignment_symbol default: \
            se_PML_tokenizer_config_default_values.attribute_id_and_assignment_symbol
        
        attribute_id_value_separator default: se_PML_tokenizer_config_default_values.attribute_id_value_separator
        
        quoted_attribute_value default: se_PML_tokenizer_config_default_values.quoted_attribute_value
        unquoted_attribute_value default: se_PML_tokenizer_config_default_values.unquoted_attribute_value

        any_char_and_space_and_attribute_id_and_assignment_symbol default: \
            se_PML_tokenizer_config_default_values.any_char_and_space_and_attribute_id_and_assignment_symbol

        text_end default: se_PML_tokenizer_config_default_values.text_end
    
        comment_snippet_end default: se_PML_tokenizer_config_default_values.comment_snippet_end

        line_continuation default: se_PML_tokenizer_config_default_values.line_continuation
        
        whitespace default: se_PML_tokenizer_config_default_values.whitespace
    .
.
