factory PML_inline_code_node_creator type:PML_node_creator<PML_inline_code_node>

    function create_node

        const text_node = i_context.require_single_text_child_node ( i_node_AST ) on_error: return_error
        return PML_inline_code_node.create ( text_node.text )
    .

    creator create kind:in_all
.
